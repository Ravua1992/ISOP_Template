$PROBLEM dummy model

$INPUT ID TIME DV CMTX EVID MDV AMT ADDL II WT
$DATA  data_run1_sim.csv IGNORE=@

$SUBROUTINE ADVAN2 TRANS2

$PK
   AVAL=WT/70

   TVCL=THETA(1) * AVAL**0.75
   CL=TVCL*EXP(ETA(1))

   TVV=THETA(2) * AVAL
   V=TVV*EXP(ETA(2))
   S2=V
   
   TVKA=THETA(3)
   KA=TVKA*EXP(ETA(3))

$ERROR 
  IPRED=F
  W=THETA(4)*IPRED
  IRES=DV-IPRED
  IWRES=IRES/W
  Y=IPRED + W*ERR(1)

$THETA
(0,8)     ;CL
(0,80)       ;V
(0,0.8)     ;KA
(0,0.3)     ; PROP ERR

$OMEGA BLOCK(2) 0.5         ; PPV_CL
                0.1 0.5         ;PPV_V
$OMEGA 0.2  ; PPV_KA

$SIGMA 1 FIX

$EST MAXEVAL=9990 PRINT=3 METH=1 INTER
$COV
$TABLE ID TIME IPRED PRED CWRES IWRES NOPRINT ONEHEADER FILE=sdtab1
$TABLE ID CL V KA ETA1 ETA2 ETA3 NOPRINT ONEHEADER FILE=patab1
$TABLE ID WT NOPRINT ONEHEADER NOAPPEND FILE=cotab1
